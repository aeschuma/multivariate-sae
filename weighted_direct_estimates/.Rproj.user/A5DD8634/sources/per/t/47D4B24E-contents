
# just a small test case to demo username/password

library(milliondeaths)

someCodes <- milliondeaths::getWbdCodes(c(
  'Injuries','Malaria','Diarrhoeal'))

# note: spatial=TRUE requires mapmisc package
# Fill in your username and password accordingly
someData = milliondeaths::mdsPoisson(
  codeList = someCodes,
  year = c(2004,2013),
  age = c(0,1,5),
  spatial=TRUE,
  verbose=TRUE,
  dbOptions = milliondeaths::milliondeathsOptions(user='username',password='password'))

head(someData$data@data)
